{
	"extends": "airbnb",
	"env": {
		"browser": true
	},
	"plugins": [
		"react"
	],
	"parser": "babel-eslint",
	"rules": {
		"strict": 0,
		"indent": [2, "tab", { "SwitchCase": 1}],
		"no-tabs": 0,
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
		"react/forbid-prop-types": 0,
		"no-console": 0,
		"comma-dangle": [2, "never"],
		"linebreak-style": 0, 
		"no-unused-vars": 0
	}
}